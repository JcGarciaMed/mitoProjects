package com.greymatter.flatmap.map;

import java.time.LocalDate;
import java.util.ArrayList;
import java.util.List;


public class Program {
	
	public static void main(String[] args) {
		List<Persona> personas = new ArrayList<>();
		
		personas.add(new Persona("Pepe","PERES", LocalDate.of(1989, 07, 02)));
		personas.add(new Persona("Jorge","PERES", LocalDate.of(1989, 07, 02)));
		personas.add(new Persona("CARLOS","PERES", LocalDate.of(1989, 07, 02)));
		personas.add(new Persona("augusto","PERES", LocalDate.of(1989, 07, 02)));
	}
}
