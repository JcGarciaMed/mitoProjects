	public static void main(String[] args) {
		List<Empleado> empleados = Arrays.asList(
				new Empleado("jorge", 7000.00, "Lima"),
				new Empleado("henry", 5000.00, "Piura"),
				new Empleado("juan", 4558.00, "Tumbes"),
				new Empleado("pepe", 1288.00, "Ciervo")				
				);
	}
}
